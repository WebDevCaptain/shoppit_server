    "nodemon": "^1.18.6",
    "prettier": "^1.15.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "pg": "^7.6.1",
    "pg-promise": "^8.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "supertest": "^3.3.0"
